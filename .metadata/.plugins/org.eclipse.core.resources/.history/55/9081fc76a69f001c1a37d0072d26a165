package POO_avanzado;

public class Animal {
	 
	public Animal() {
		
		System.out.println("Creación de un Animal");
	 }

	public class Hipopotamo extends Animal {
		 public Hipopotamo() {
		 System.out.println("Creación de un Hipopótamo");
		 }
		}


	 public static void main (String[] args) {
		 Hipopotamo hipo1 = new Hipopotamo();
}
	 }