package POO;

public class Punto {

	private int X;
	private int Y;
	
	
	public Punto(int x, int y) {
		super();
		this.X = x;
		this.Y = y;
	}


	public int getX() {
		return X;
	}


	public void setX(int x) {
		X = x;
	}


	public int getY() {
		return Y;
	}


	public void setY(int y) {
		Y = y;
	}

	public int cuadrante() {
		if(this.X >0 && this.Y > 0) {
			return 1;
		}else if(this.X <>0 && this.Y > 0) {
			return 2;
		}
		
		
	}
	public static void main(String[] args) {
		

	}

}
